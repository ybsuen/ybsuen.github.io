<?xml version='1.0' encoding='utf-8'?>
<widget id="com.intechnigence.lesson4" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>lesson4</name>
    <description>
        Lesson 4 Class Demonstration
    </description>
    <author email="bernard@cuhk.edu.hk" href="https://github.com/ybsuen">
        Bernard Suen
    </author>
    <content src="index.html" />
    <preference name="BackupWebStorage" value="local"/>
    <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~1.3.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <platform name="android">
        <icon density="mdpi" src="www/res/icon/android/ic_launcher-mdpi.png" />
        <icon density="hdpi" src="www/res/icon/android/ic_launcher-hdpi.png" />
        <icon density="xhdpi" src="www/res/icon/android/ic_launcher-xhdpi.png" />
